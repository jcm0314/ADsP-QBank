generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Question {
  id          String   @id @default(cuid())
  title       String
  content     String
  category    String
  difficulty  Int
  options     String
  answer      Int
  explanation String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}